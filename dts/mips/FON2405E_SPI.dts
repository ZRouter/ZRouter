/dts-v1/;

#include <dt-bindings/gpio/tegra-gpio.h>
//#include "rt3050.dtsi"
#include "fbsd-rt3050.dtsi"

/ {
	compatible = "FON2405E", "ralink,rt3050-soc";
	model = "FON FON2405E";

	gpio-leds {
		compatible = "gpio-leds";

		status {
			label = "status";
			gpios = <&gpio0 14 0>;
		};

		led1 {
			label = "led1";
			gpios = <&gpio0 11 0>;
		};

		led2 {
			label = "led2";
			gpios = <&gpio0 9 1>;
		};

		led3 {
			label = "led3";
			gpios = <&gpio0 12 1>;
		};

	};
	gpio-keys-polled {
		compatible = "gpio-keys-polled";
		#address-cells = <1>;
		#size-cells = <0>;
		poll-interval = <20>;

		reset {
			label = "reset";
			gpios = <&gpio0 10 1>;
			linux,code = <0x198>;
		};

	};
};

&spi0 {
	status = "okay";

	m25p80@0 {
		#address-cells = <1>;
		#size-cells = <1>;
		compatible = "jedec,spi-nor";
		reg = <0>;
		linux,modalias = "m25p80", "mx25l3205d";
		spi-max-frequency = <80000000>;

		partition@0 {
			label = "u-boot";
			reg = <0x0 0x10000>;
			read-only;
		};

		partition@10000 {
			label = "u-boot-env";
			reg = <0x10000 0x10000>;
			read-only;
		};

		kernel: partition@20000 {
			label = "kernel";
			reg = <0x20000 0x100000>;
		};
		rootfs: partition@100000 {
			label = "rootfs";
			reg = <0x00120000 0x006e0000>;
		};
/*
		partition@1300000 {
			compatible = "denx,uimage";
			label = "firmware";
			reg = <0x20000 0x7e0000>;
		};
*/
		partition@800000 {
			label = "upgrade";
			reg = <0x20000 0x7e0000>;
		};
	};
};

&pinctrl {
	state_default: pinctrl0 {
		gpio {
			ralink,group = "i2c", "jtag", "uartf", "rgmii", "mdio";
			ralink,function = "gpio";
		};

	};
};

&ethernet {
//	mtd-mac-address = <&factory 0x28>;
};

&esw {
	mediatek,portmap = <0x2f>;
};

&wmac {
//	ralink,mtd-eeprom = <&factory 0>;
};

