*/

&spi1 {
	status = "okay";
	cs-gpios = <&gpio0 9 GPIO_ACTIVE_LOW>;
	non-flash;
	spigen@0 {
		compatible = "freebsd,spigen";
		reg = <0>;
		spi-max-frequency = <80000000>;	// SPICLK = 1
		status = "okay";
	};
