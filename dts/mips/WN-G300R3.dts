/dts-v1/;

#include "mt7620n.dtsi"

/ {
	/* same as Sitecom WLR-3100 ? */
	compatible = "ralink,mt7620n-soc";
	model = "IO Data WN-G300R3";

	gpio-leds {
		compatible = "gpio-leds";

		/* LED8 GPIO#39 (power)*/
		status {
			label = "status";
			gpios = <&gpio1 15 1>;
		};

		/* LED2 GPIO#38 */
		wps {
			label = "wps";
			gpios = <&gpio1 14 1>;
		};

	};

	gpio-keys-polled {
		compatible = "gpio-keys-polled";
		#address-cells = <1>;
		#size-cells = <0>;
		poll-interval = <20>;

		reset {
			label = "reset";
			gpios = <&gpio0 1 1>;
			linux,code = <0x198>;
		};

		wps {
			label = "wps";
			gpios = <&gpio0 2 1>;
			linux,code = <0x211>;
		};

	};
};

&gpio1 {
	status = "okay";
};

&gpio2 {
	status = "okay";
};

&spi0 {
	status = "okay";

	m25p80@0 {
		#address-cells = <1>;
		#size-cells = <1>;
		compatible = "jedec,spi-nor";
		reg = <0>;
		linux,modalias = "m25p80", "s25fl064k";
		spi-max-frequency = <10000000>;

		partition@0 {
			label = "u-boot";
			reg = <0x0 0x30000>;
			read-only;
		};

		partition@30000 {
			label = "u-boot-env";
			reg = <0x30000 0x10000>;
			read-only;
		};

		factory: partition@40000 {
			label = "factory";
			reg = <0x40000 0x10000>;
			read-only;
		};
/*
		partition@50000 {
			label = "firmware";
			reg = <0x50000 0x7b0000>;
		};
*/

		partition@50000 {
			label = "kernel";
			reg = <0x50000 0x100000>;
		};

		partition@110000 {
			label = "rootfs";
			reg = <0x150000 0x2b0000>;
		};

		partition@800000 {
			label = "upgrade";
			reg = <0x50000 0x3b0000>;
		};
	};
};

&pinctrl {
	state_default: pinctrl0 {
		gpio {
			ralink,group = "ephy", "wled", "pa", "i2c", "wdt", "uartf";
			ralink,function = "gpio";
		};

	};
};

&ethernet {
	mtd-mac-address = <&factory 0x4>;
	mediatek,portmap = "wllll";
};

&wmac {
	ralink,mtd-eeprom = <&factory 0>;
};

